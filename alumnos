--creacion de la tabla alumnos 
CREATE TABLE IF NOT EXISTS alumnos (matricula VARCHAR(10) PRIMARY KEY, nombre VARCHAR(100) NOT NULL, apellido_p VARCHAR(100) NOT NULL,
                                   edad INT NOT NULL, 
                                   genero CHAR NOT NULL,
trabajo BOOLEAN NOT NULL);

